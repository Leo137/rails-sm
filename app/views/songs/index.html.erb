<p id="notice"><%= notice %></p>

<h1>Songs</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Server difficulty name</th>
      <th>Server difficulty number</th>
      <th>Server</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.name %></td>
        <td><%= song.server_difficulty_name %></td>
        <td><%= song.server_difficulty_number %></td>
        <td><%= song.server_id %></td>
        <td><%= link_to 'Show', song %></td>
        <td><%= link_to 'Edit', edit_song_path(song) %></td>
        <td><%= link_to 'Destroy', song, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Song', new_song_path %>
